<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.android.mi.wearable.watchface5.view.IndicatorView
        android:id="@+id/indicatorView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@android:color/transparent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:focusable="true"
        app:iv_anchor_color_level_1="@color/color1"
        app:iv_anchor_color_level_2="@color/color2"
        app:iv_anchor_color_level_3="@color/color3"
        app:iv_anchor_color_level_5="@color/color5"
        app:iv_anchor_color_level_6="@color/color6"
        app:iv_anchor_color_level_7="@color/color7"
        app:iv_anchor_color_level_8="@color/color8"
        app:iv_anchor_color_level_9="@color/color9"
        app:iv_anchor_color_level_10="@color/color10"
        app:iv_anchor_color_level_11="@color/color11"
        app:iv_anchor_color_level_12="@color/color12"
        app:iv_anchor_color_level_13="@color/color13"
        app:iv_anchor_color_level_14="@color/color14"
        app:iv_anchor_color_level_15="@color/color15"
        app:iv_anchor_color_level_16="@color/color16"
        app:iv_anchor_color_level_17="@color/color17"
        app:iv_anchor_color_level_18="@color/color18"
        app:iv_anchor_color_level_19="@color/color19"
        app:iv_anchor_color_level_20="@color/color20"
        app:iv_anchor_color_level_21="@color/color21"
        app:iv_anchor_color_level_22="@color/color22"
        app:iv_anchor_color_level_23="@color/color23"
        app:iv_anchor_color_level_24="@color/color24"
        app:iv_anchor_color_level_25="@color/color25"
        app:iv_anchor_color_level_26="@color/color26"
        app:iv_anchor_color_level_27="@color/color27"
        app:iv_anchor_color_level_28="@color/color28"
        app:iv_anchor_color_level_29="@color/color29"
        app:iv_anchor_color_level_30="@color/color30"
        app:iv_anchor_color_level_31="@color/color31"
        app:iv_anchor_color_level_32="@color/color32"
        app:iv_anchor_color_level_33="@color/color33"
        app:iv_anchor_color_level_34="@color/color34"
        app:iv_anchor_color_level_35="@color/color35"
        app:iv_anchor_color_level_36="@color/color36"
        app:iv_anchor_is_colorful="true"
        app:iv_anchor_size_level_1="13"
        app:iv_anchor_size_level_2="11"
        app:iv_dot_interval_angle="12"
        app:iv_ring_stroke="2px"
        app:iv_dot_count="36"
        app:iv_ring_angle="0"
        app:iv_ring_radius="18" />

    <TextView
        android:id="@+id/tv_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="10dp"
        android:gravity="center_horizontal"
        android:textColor="@android:color/white"
        android:textSize="16sp"
        android:fontFamily="sans-serif-black"
        android:text="Color"
        />


</androidx.constraintlayout.widget.ConstraintLayout>

